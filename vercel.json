{
  "version": 2,
  "builds": [
    {
      "src": "index.js",  // Adjust to the correct entry point file name (your main server file)
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/index.js" }  // This directs all API requests to your main server file
  ]
}
